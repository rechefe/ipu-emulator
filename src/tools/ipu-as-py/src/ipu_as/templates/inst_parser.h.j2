#ifndef INST_PARSER_H
#define INST_PARSER_H

#include <stdint.h>

    {% for enum in enums %}
    typedef enum {
        {% for value in enum.values %}
            {{ value.name }} = {{ value.value }}{% if not loop.last %},{% endif %}
        {% endfor %}
    } inst_parser__{{ enum.name }}_t;
    {% endfor %}

    typedef struct {
        {% for name, bits in inst_bit_fields %}
            inst_parser__inst_t {{ name }} : {{ bits }};
        {% endfor %}
    } inst_parser__inst_t;

#endif
